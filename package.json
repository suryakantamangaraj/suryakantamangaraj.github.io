{
  "name": "surya-raj",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/portfolio-v2/browser/stats.json",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.16",
    "@angular/common": "^20.3.16",
    "@angular/compiler": "^20.3.16",
    "@angular/core": "^20.3.16",
    "@angular/forms": "^20.3.16",
    "@angular/platform-browser": "^20.3.16",
    "@angular/platform-browser-dynamic": "^20.3.16",
    "@angular/router": "^20.3.16",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "^20.3.16",
    "@angular-eslint/eslint-plugin": "16.0.0",
    "@angular-eslint/eslint-plugin-template": "16.0.0",
    "@angular-eslint/template-parser": "16.0.0",
    "@angular/cli": "^20.3.16",
    "@angular/compiler-cli": "^20.3.16",
    "@angular/localize": "^20.3.16",
    "@cypress/schematic": "^3.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "cypress": "^13.17.0",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-preset-angular": "^14.5.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.3.0",
    "postcss-scss": "^4.0.9",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.4.2",
    "stylelint": "^16.13.2",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.10.0",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "src/**/*.{ts,scss,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^6.0.0"
  },
  "engines": {
    "node": ">=18.19.1"
  }
}
